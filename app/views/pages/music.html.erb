<% provide :title, "Music" %>
<% provide :meta_description, "Tunes and mixtapes by Brock Golden, who's also known as – GOLDNGRAMS. Here you'll find my house and techno makes and mixes on Youtube, Soundcloud, and Twitter." %>
<div class="row">
	<div class="col-md-10 col-md-offset-1 col-xs-10 col-xs-offset-1" id="posts-container">
		<% @posts.each do |p| %>
			<%= render partial: "posts/post_preview", locals: { post: p } %>
		<% end %>
		<% will_paginate @posts %>
	<div>
</div>
